{
  "name": "app",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=production node -r dotenv/config dist/server.js",
    "start:services": "NODE_ENV=production node -r dotenv/config dist/services.js",
    "dev": "rm -rf src/public/* && run-p dev:server dev:parcel",
    "dev:server": "NODE_ENV=development ts-node-dev --ignore-watch src/public,src/client -r dotenv/config src/server.ts",
    "dev:parcel": "parcel watch src/client/html/*.html --dist-dir src/public --public-url /public",
    "dev:services": "NODE_ENV=development ts-node-dev --ignore-watch -r dotenv/config src/services.ts",
    "build:docker:service": "NODE_ENV=production tsc -p tsconfig.json",
    "build:parcel": "rm -rf .parcel-cache && npm run build:parcel:login && npm run build:parcel:homepage && npm run build:parcel:dashboard && npm run build:parcel:discovery && npm run build:parcel:go-to-serp && npm run build:parcel:topic-map",
    "build:docker:parcel": "npm run build:parcel:login && npm run build:parcel:homepage && npm run build:parcel:dashboard && npm run build:parcel:discovery",
    "build:parcel:login": "parcel build src/client/html/login.html --no-source-maps --dist-dir dist/public --public-url /public",
    "build:parcel:homepage": "parcel build src/client/html/homepage.html --no-source-maps --dist-dir dist/public --public-url /public",
    "build:parcel:dashboard": "parcel build src/client/html/dashboard.html --no-source-maps --dist-dir dist/public --public-url /public",
    "build:parcel:discovery": "parcel build src/client/html/discovery.html --no-source-maps --dist-dir dist/public --public-url /public",
    "build:parcel:topic-map": "parcel build src/client/html/topic-map.html --no-source-maps --dist-dir dist/public --public-url /public",
    "build:parcel:go-to-serp": "parcel build src/client/html/go-to-serp.html --no-source-maps --dist-dir dist/public --public-url /public",
    "build": "rm -rf dist && mkdir -p dist/public && NODE_ENV=production tsc -p tsconfig.json && cp -r src/data-sources dist/ && cp -r src/py dist/ && npm run build:parcel",
    "gcp-build": "rm -rf dist && mkdir -p dist/public && NODE_ENV=production tsc -p tsconfig.json && cp -r src/data-sources dist/ && cp -r src/py dist/ && npm run build:parcel",
    "script": "ts-node --"
  },
